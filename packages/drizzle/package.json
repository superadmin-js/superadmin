{
    "name": "@superadmin/drizzle",
    "repository": "https://github.com/kedrzu/superadmin.git",
    "author": "Michał Kędrzyński <m.kedrzynski@gmail.com>",
    "version": "1.0.0",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./*.js": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js",
            "source": "./src/*.ts",
            "default": "./dist/*.js"
        }
    },
    "sideEffects": false,
    "scripts": {
        "lint": "prettier ./src --write && eslint . --fix --cache"
    },
    "dependencies": {
        "@superadmin/core": "*",
        "drizzle-orm": "0.45.1"
    },
    "devDependencies": {
        "@superadmin/tsconfig": "*",
        "@types/node": "25.2.3",
        "eslint": "9.39.2",
        "prettier": "3.8.1",
        "vitest": "4.0.18"
    }
}
