{
    "name": "@superadmin/server",
    "repository": "https://github.com/kedrzu/superadmin.git",
    "author": "Michał Kędrzyński <m.kedrzynski@gmail.com>",
    "version": "1.0.0",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "scripts": {
        "eslint": "eslint . --fix --cache"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./entry": {
            "types": "./dist/entry.d.ts",
            "import": "./dist/entry.js",
            "default": "./dist/entry.js"
        },
        "./entry-dev": {
            "types": "./dist/entry-dev.d.ts",
            "import": "./dist/entry-dev.js",
            "default": "./dist/entry-dev.js"
        },
        "./module": {
            "types": "./dist/module.d.ts",
            "import": "./dist/module.js",
            "default": "./dist/module.js"
        }
    },
    "dependencies": {
        "@nzyme/crypto": "^0.14.0",
        "@nzyme/fetch-utils": "^0.14.0",
        "@nzyme/ioc": "^0.14.0",
        "@nzyme/logging": "^0.14.0",
        "@nzyme/rollup-utils": "^0.14.0",
        "@nzyme/rpc": "^0.14.0",
        "@nzyme/types": "^0.14.0",
        "@nzyme/utils": "^0.14.0",
        "@superadmin/core": "*",
        "@superadmin/runtime-common": "*",
        "@superadmin/schema": "*",
        "@superadmin/utils": "*",
        "@superadmin/validation": "*",
        "@zod/mini": "4.0.0-beta.20250505T195954",
        "chalk": "^5.6.2",
        "connect": "^3.7.0",
        "jose": "^6.1.3"
    },
    "devDependencies": {
        "@superadmin/tsconfig": "*",
        "@types/connect": "^3.4.38",
        "@types/node": "^25.0.3",
        "typescript": "^5.9.3",
        "vitest": "^4.0.16"
    }
}
