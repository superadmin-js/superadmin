{
    "name": "@superadmin/cli",
    "repository": "https://github.com/kedrzu/superadmin.git",
    "author": "Michał Kędrzyński <m.kedrzynski@gmail.com>",
    "version": "1.0.0",
    "type": "module",
    "bin": {
        "superadmin": "./bin/cli.js"
    },
    "watch": {
        "manifest": "dist/**/*.js"
    },
    "scripts": {
        "eslint": "eslint . --fix --cache",
        "build": "tsc --build"
    },
    "dependencies": {
        "@nzyme/ioc": "*",
        "@nzyme/logging": "*",
        "@nzyme/node-utils": "*",
        "@nzyme/project-utils": "*",
        "@nzyme/utils": "*",
        "@oclif/core": "^4.0.29",
        "@oclif/plugin-help": "^6.2.15",
        "@oclif/plugin-version": "^2.2.15",
        "arg": "^5.0.2",
        "chalk": "^5.3.0",
        "consola": "^3.2.3",
        "del": "^7.1.0",
        "dotenv": "^16.4.5",
        "drizzle-orm": "0.32.0",
        "is-ci": "^3.0.1",
        "simple-git": "^3.25.0",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.2"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/is-ci": "^3.0.4",
        "@types/mime-types": "^2.1.4",
        "@types/node": "^22.7.5",
        "@types/source-map-support": "^0.5.10",
        "nodemon": "^3.1.4",
        "npm-watch": "^0.12.0",
        "oclif": "^4.13.12"
    },
    "files": [
        "/bin/cli.js",
        "/dist"
    ],
    "oclif": {
        "bin": "superadmin",
        "commands": {
            "strategy": "explicit",
            "target": "./dist/commands.js"
        },
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-version"
        ],
        "topicSeparator": " "
    },
    "depcheck": {
        "ignoreDeps": [
            "@oclif/plugin-*"
        ]
    }
}
