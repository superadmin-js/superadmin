{
    "name": "@superadmin/devkit",
    "version": "1.0.0",
    "repository": "https://github.com/kedrzu/superadmin.git",
    "author": "Michał Kędrzyński <m.kedrzynski@gmail.com>",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "scripts": {
        "lint": "prettier ./src --write && eslint . --fix --cache"
    },
    "dependencies": {
        "@nzyme/esm": "0.14.0",
        "@nzyme/ioc": "0.14.0",
        "@nzyme/project-utils": "0.14.0",
        "@nzyme/rollup-utils": "0.14.0",
        "@nzyme/utils": "0.14.0",
        "@rollup/plugin-alias": "6.0.0",
        "@rollup/plugin-commonjs": "29.0.0",
        "@rollup/plugin-json": "6.1.0",
        "@rollup/plugin-node-resolve": "16.0.3",
        "@rollup/plugin-typescript": "12.3.0",
        "@rollup/plugin-virtual": "3.0.2",
        "@superadmin/client": "*",
        "@superadmin/config": "*",
        "@superadmin/server": "*",
        "@tailwindcss/vite": "4.1.17",
        "@vitejs/plugin-vue": "6.0.4",
        "@vitejs/plugin-vue-jsx": "5.1.4",
        "autoprefixer": "10.4.23",
        "chalk": "5.6.2",
        "chokidar": "4.0.3",
        "debug": "4.4.3",
        "fast-glob": "3.3.3",
        "lodash.debounce": "4.0.8",
        "postcss": "8.5.6",
        "rollup": "4.46.2",
        "rollup-plugin-sourcemaps": "0.6.3",
        "type-fest": "5.4.3",
        "typescript": "5.9.3",
        "ufo": "1.6.3",
        "vite": "7.3.1",
        "vite-plugin-checker": "0.12.0",
        "vite-plugin-node-polyfills": "0.25.0",
        "vite-tsconfig-paths": "6.0.5",
        "vue-tsc": "3.2.4"
    },
    "devDependencies": {
        "@nzyme/eslint": "0.14.0",
        "@superadmin/tsconfig": "*",
        "@types/debug": "4.1.12",
        "@types/lodash.debounce": "4.0.9",
        "@types/node": "25.2.0",
        "eslint": "9.39.2",
        "prettier": "3.8.1"
    },
    "depcheck": {
        "ignoreDeps": [
            "vue-tsc"
        ]
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./*.js": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js",
            "default": "./dist/*.js",
            "source": "./src/*.ts"
        }
    }
}
