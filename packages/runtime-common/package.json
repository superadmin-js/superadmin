{
    "name": "@superadmin/runtime-common",
    "version": "1.0.0",
    "repository": "https://github.com/kedrzu/superadmin.git",
    "author": "Michał Kędrzyński <m.kedrzynski@gmail.com>",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "scripts": {
        "lint": "prettier ./src --write && eslint . --fix --cache"
    },
    "exports": {
        ".": "./dist/index.js",
        "./*.js": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js",
            "default": "./dist/*.js",
            "source": "./src/*.ts"
        }
    },
    "dependencies": {
        "@nzyme/ioc": "^0.14.0",
        "@nzyme/types": "^0.14.0",
        "@nzyme/utils": "^0.14.0",
        "@superadmin/core": "*",
        "@superadmin/schema": "*",
        "debug": "^4.4.3"
    },
    "devDependencies": {
        "@superadmin/tsconfig": "*",
        "@types/debug": "^4.1.12",
        "@types/node": "^25.2.0",
        "eslint": "^9.39.2",
        "prettier": "^3.8.1",
        "vitest": "^4.0.18"
    }
}
