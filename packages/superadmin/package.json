{
    "name": "superadmin",
    "version": "1.0.0",
    "repository": "https://github.com/kedrzu/superadmin.git",
    "author": "Michał Kędrzyński <m.kedrzynski@gmail.com>",
    "type": "module",
    "main": "./dist/core.js",
    "module": "./dist/core.js",
    "types": "./dist/core.d.ts",
    "sideEffects": false,
    "scripts": {
        "eslint": "eslint . --fix --cache"
    },
    "bin": "./dist/cli.js",
    "exports": {
        ".": {
            "default": "./dist/core.js",
            "types": "./dist/core.d.ts"
        },
        "./client": {
            "default": "./dist/client.js",
            "types": "./dist/client.d.ts"
        },
        "./server": {
            "default": "./dist/server.js",
            "types": "./dist/server.d.ts"
        },
        "./schema": {
            "default": "./dist/schema.js",
            "types": "./dist/schema.d.ts"
        },
        "./validation": {
            "default": "./dist/validation.js",
            "types": "./dist/validation.d.ts"
        },
        "./ioc": {
            "default": "./dist/ioc.js",
            "types": "./dist/ioc.d.ts"
        },
        "./config": {
            "default": "./dist/config.js",
            "types": "./dist/config.d.ts"
        }
    },
    "dependencies": {
        "@nzyme/cli": "^0.14.0",
        "@nzyme/ioc": "^0.14.0",
        "@nzyme/project-utils": "^0.14.0",
        "@nzyme/utils": "^0.14.0",
        "@superadmin/config": "*",
        "@superadmin/devkit": "*",
        "@superadmin/tsconfig": "*",
        "ts-node": "^10.9.2"
    },
    "devDependencies": {
        "@nzyme/eslint": "^0.14.0",
        "@types/node": "^24.6.2"
    }
}
