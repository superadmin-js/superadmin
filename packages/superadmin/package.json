{
    "name": "superadmin",
    "version": "1.0.0",
    "repository": "https://github.com/kedrzu/superadmin.git",
    "author": "Michał Kędrzyński <m.kedrzynski@gmail.com>",
    "type": "module",
    "main": "./dist/core.js",
    "module": "./dist/core.js",
    "types": "./dist/core.d.ts",
    "sideEffects": false,
    "scripts": {
        "lint": "prettier ./src --write && eslint . --fix --cache"
    },
    "bin": "./dist/cli.js",
    "exports": {
        ".": {
            "types": "./dist/core.d.ts",
            "default": "./dist/core.js"
        },
        "./client": {
            "types": "./dist/client.d.ts",
            "default": "./dist/client.js"
        },
        "./server": {
            "types": "./dist/server.d.ts",
            "default": "./dist/server.js"
        },
        "./schema": {
            "types": "./dist/schema.d.ts",
            "default": "./dist/schema.js"
        },
        "./validation": {
            "types": "./dist/validation.d.ts",
            "default": "./dist/validation.js"
        },
        "./ioc": {
            "types": "./dist/ioc.d.ts",
            "default": "./dist/ioc.js"
        },
        "./config": {
            "types": "./dist/config.d.ts",
            "default": "./dist/config.js"
        }
    },
    "dependencies": {
        "@nzyme/cli": "^0.14.0",
        "@nzyme/ioc": "^0.14.0",
        "@nzyme/project-utils": "^0.14.0",
        "@nzyme/utils": "^0.14.0",
        "@superadmin/client": "workspace:*",
        "@superadmin/config": "*",
        "@superadmin/core": "workspace:*",
        "@superadmin/devkit": "*",
        "@superadmin/ioc": "workspace:*",
        "@superadmin/schema": "workspace:*",
        "@superadmin/server": "workspace:*",
        "@superadmin/tsconfig": "*",
        "@superadmin/validation": "workspace:*"
    },
    "devDependencies": {
        "@nzyme/eslint": "^0.14.0",
        "@types/node": "^25.2.0",
        "eslint": "^9.39.2",
        "prettier": "^3.8.1"
    }
}
